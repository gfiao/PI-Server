<h1>Listing transport_hours</h1>

<table>
  <thead>
    <tr>
      <th>Transport</th>
      <th>Hour</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transport_hours.each do |transport_hour| %>
      <tr>
        <td><%= transport_hour.transport_id %></td>
        <td><%= transport_hour.hour_id %></td>
        <td><%= link_to 'Show', transport_hour %></td>
        <td><%= link_to 'Edit', edit_transport_hour_path(transport_hour) %></td>
        <td><%= link_to 'Destroy', transport_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transport hour', new_transport_hour_path %>
