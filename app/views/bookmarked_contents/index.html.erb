<h1>Listing bookmarked_contents</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookmarked_contents.each do |bookmarked_content| %>
      <tr>
        <td><%= bookmarked_content.user_id %></td>
        <td><%= bookmarked_content.content_id %></td>
        <td><%= link_to 'Show', bookmarked_content %></td>
        <td><%= link_to 'Edit', edit_bookmarked_content_path(bookmarked_content) %></td>
        <td><%= link_to 'Destroy', bookmarked_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bookmarked content', new_bookmarked_content_path %>
